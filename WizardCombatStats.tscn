[gd_scene load_steps=2 format=2]

[sub_resource type="GDScript" id=1]

script/source = "extends PanelContainer

onready var name_label = $VBoxContainer/HBoxContainer/WizName
onready var health_label = $VBoxContainer/HBoxContainer/PanelContainer/Label
onready var runes = $VBoxContainer/Runes.get_children()

func update_with_wizard(wizard):
	name_label.text = wizard.wiz_name
	health_label.text = str(wizard.health) + \"/10\"
	for r in runes:
		var label = r.get_child(0).get_child(0)
		label.text = \"\"
	var i = -1
	for r in wizard.getRunesSoFar():
		var label = runes[i].get_child(0).get_child(0)
		label.text = r
		i -= 1
"

